

@{
    ViewBag.Title = "Index";
}

@if (ViewBag.displayMenu == "Yes")
{
    <h1>Welcome Admin. Now you can create user Role.</h1>
    <h3>
        <li>@Html.ActionLink("Create Questionaire", "Index", "Questionaire")</li>
        <li>@Html.ActionLink("View Survey", "ViewSurvey", "Questionaire")</li>
